{
    "name": "aminireza-ir/laravel-easyblade",
    "license": "MIT",
    "description": "Create a easier & readable Blade view with EasyBlade",
    "authors": [
        {
            "name": "Reza Amini",
            "email": "rezaaminiroyal@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "laravel/framework": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "EasyBlade\\" : "src/"
        }
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "EasyBlade\\EasyBladeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
